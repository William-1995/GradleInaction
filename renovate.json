{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "baseBranches": [
      "master"
    ],
  "fileMatch": [
    "\\.gradle(\\.kts)?$",
    "(^|/)gradle\\.properties$",
    "(^|/)gradle/.+\\.toml$",
    "(^|/)buildSrc/.+\\.kt$",
    "\\.versions\\.toml$",
    "(^|/)versions.props$",
    "(^|/)versions.lock$"
  ]
}
